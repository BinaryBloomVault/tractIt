{
  "expo": {
    "name": "tractIt",
    "slug": "tractIt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/friendsReload.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/friendsReload.png",
      "resizeMode": "contain",
      "backgroundColor": "#A9DFBF"
    },
    "assetBundlePatterns": ["assets/fonts/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.binarybloom.tractit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.binarybloom.tractit"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "SpaceMono": ["./assets/fonts/SpaceMono-Regular.ttf"],
          "Gudea-Bold": ["./assets/fonts/Gudea-Bold.ttf"],
          "Gudea-Regular": ["./assets/fonts/Gudea-Regular.ttf"],
          "Cabin-Regular": ["./assets/fonts/Cabin-Regular.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9a0d152b-8074-4e75-bcc1-d78b340ac3e2"
      }
    }
  }
}
